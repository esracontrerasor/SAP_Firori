<core:FragmentDefinition 
controllerName="com.inv.sapfiroriwebinversion.controller.security.pages.SecurityTable"
xmlns="sap.m" 
xmlns:core="sap.ui.core">
     <Dialog
        id="DeleteConfirmationDialog"
        title="Confirmar eliminación"
        type="Message"
        draggable="true"
        resizable="true">
        <content>
            <Text id="_IDGenText1" text="¿Está seguro de que desea eliminar este usuario?" />
        </content>
        <beginButton>
            <Button id="_IDGenButton1"
                text="Eliminar"
                type="Reject"
                press="onConfirmDelete" />
        </beginButton>
        <endButton>
            <Button id="_IDGenButton2"
                text="Cancelar"
                press="onCancelDelete" />
        </endButton>
    </Dialog>
</core:FragmentDefinition>